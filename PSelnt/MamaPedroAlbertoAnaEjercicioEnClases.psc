Proceso MamaPedroAlbertoAnaEjercicioEnClases
	Definir mama, pedro, alberto, ana Como Entero;
	mama<-0; pedro<-0; alberto<-0; ana<-0;
	Escribir "La edad de pedro";
	Leer pedro;
	alberto<-pedro*(2/3);
	ana<-pedro*(4/3);
	mama<-pedro+alberto+ana;
	Escribir "la edad de pedro es", pedro;
	Escribir "la edad de pedro es", alberto;
	Escribir "la edad de pedro es", ana;
	Escribir "la edad de la mama es", mama;
FinProceso
